     ]
    }
   ],
   "source": [
    "#custom exceptions\n",
    "class AgeError(Exception):\n",
    "    pass\n",
    "\n",
    "def set(age):\n",
    "    if age<0:\n",
    "        raise AgeError(\"Age cannot be negative.\")\n",
    "    print(f\"Age set to {age}\")\n",
    "try:\n",
    "    set(-5)\n",
    "except AgeError as e:\n",
    "    print(e)"
   ]
